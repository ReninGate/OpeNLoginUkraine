Messages:
  Title:
    # Повідомлення надсилається перед входом у систему
    before-login:
      title: "&a&lЛаскаво просимо!"
      subtitle: "&7Використання: /login <пароль>"
      delays:
        start: 0
        duration: 9999
        end: 6
    # Повідомлення надсилається перед реєстрацією
    before-register:
      title: "&a&lЛаскаво просимо!"
      subtitle: "&7Використання: /register <пароль> <повтор пароля>"
      delays:
        start: 0
        duration: 9999
        end: 6
    # Повідомлення надсилається після успішного входу в систему
    after-login:
      title: ""
      subtitle: "&aВи успішно увійшли!"
      delays:
        start: 0
        duration: 60
        end: 6
    # Повідомлення надсилається після успішної реєстрації
    after-register:
      title: ""
      subtitle: "&aВи успішно зареєструвалися!"
      delays:
        start: 0
        duration: 60
        end: 6

  delay-kick:
    # Повідомлення надсилається, коли гравець занадто довго реєструється
    register-kick: "&cЧас реєстрації минув."

    # Повідомлення надсилається, коли гравець занадто довго входить в акаунт
    login-kick: "&cВи занадто довго входили в акаунт."

  successful-operations:
    # Повідомлення надсилається, коли гравець змінює свій пароль
    password-changed: "&aВаш пароль було успішно змінено."

    # Повідомлення надсилається, коли гравець успішно входить в акаунт
    successful-login: "&aВи успішно увійшли в акаунт."

    # Повідомлення надсилається, коли гравець успішно реєструється
    successful-register: "&aВи успішно зареєструвалися."

    # Повідомлення надсилається, коли гравець видаляє свій акаунт
    unregister-kick: "&cРеєстрацію акаунта успішно відкликано."

  kick-messages:
    # Повідомлення надсилається, коли гравець намагається зайти на сервер із маленькою різницею в ніку
    nick-already-registered: "&cВи не можете увійти з ніком {0}&r\n&r&сЦе ім'я користувача вже зареєстровано як {1}&r\n&r\n&r&сДля входу ви маєте використати свій нікнейм без прописних/рядкових літер"

    # Повідомлення надсилається, коли гравець занадто багато разів вводить неправильний пароль
    failed-many-times: "&cВи занадто багато разів ввели неправильний пароль."

    # Повідомлення надсилається, коли гравець вводить неправильний пароль
    incorrect-password: "&cНеправильний пароль! Спробуйте знову."

  error-messages:
    # Повідомлення надіслано, коли ви вже увійшли в систему.
    already-login: "&cВи вже авторизовані."

    # Повідомлення надсилається, коли нік уже зареєстрований.
    already-registered: "&cЦе ім'я користувача вже зареєстровано."

    # Повідомлення надсилається, коли нік ще не зареєстрований.
    not-registered: "&cЦей нік ще не зареєстрований."

    # Повідомлення надсилається, коли перший пароль не відповідає другому паролю
    passwords-dont-match: "&cПаролі не збігаються."

    # Повідомлення надсилається, коли гравець намагається змінити пароль, вводячи той самий пароль, що й поточний
    password-same-as-old: "&cВведений пароль збігається з поточним."

    # Повідомлення надсилається, коли гравець вводить занадто довгий пароль
    password-too-large: "&cВи ввели занадто довгий пароль."

    # Повідомлення надсилається, коли гравець вводить занадто короткий пароль
    password-too-small: "&cВи ввели занадто короткий пароль."

    # Повідомлення надсилається, коли у гравця недостатньо прав для використання будь-якої команди
    insufficient-permissions: "&cУ вас немає прав для використання цієї команди."

    # Повідомлення виводиться, коли гравця викидає з серверу через те, що він уже зайшов на сервер
    already-online: "&cЦей гравець уже зайшов на сервер."

    # Повідомлення надсилається, якщо цю команду не можна виконати в консолі
    player-command-usage: "&cЦю команду не можна використовувати в консолі."

    # Повідомлення надсилається після перезавантаження плагіна
    plugin-reload-message: "&eOpenLogin успішно перезавантажено."

    # Повідомлення надсилається, якщо операція з базою даних не була успішно виконана
    database-error: "&cНа жаль, нам не вдалося виконати цю операцію. Будь ласка, спробуйте ще раз. [SQL FAIL]"

  other-messages:
    # Повідомлення для використання команди /login
    message-login: "&eВведіть: &7/login <пароль>&e."

    # Повідомлення для використання команди /register
    message-register: "&eВведіть: &7/register <пароль> <повтор пароля>&e."

    # Повідомлення для команди зміни пароля /changepassword
    message-changepassword: "&eВведіть: &7/changepassword <поточний пароль> <новий пароль>&e."

    # Повідомлення надіслано для використання команди /unregister
    message-unregister: "&eВикористовуйте команду &7/unregister <поточний пароль>&e."
